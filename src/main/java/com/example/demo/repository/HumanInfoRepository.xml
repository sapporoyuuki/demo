<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.repository.HumanInfoRepository">
    <select id="select" resultType="com.example.demo.dto.HumanInfoDto">
        SELECT 
            name,
            age,
            feature
        FROM 
            human_info
        WHERE
            name = #{name}
    </select>
</mapper>
